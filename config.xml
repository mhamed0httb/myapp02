<?xml version='1.0' encoding='utf-8'?>
<widget id="com.HattabCompany.myapp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>myapp02</name>
    <description>
        A second sample Apache Cordova application made by mhamed
    </description>
    <author email="mhamed.httb@yahoo.fr" href="">
        Mhamed Hattab
    </author>
    <content src="index.html" />
	<icon src="img/samapng.png" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
